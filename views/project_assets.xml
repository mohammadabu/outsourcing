<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <template id="assets_backend" name="outsourcing assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/outsourcing/static/src/css/outsourcing.css"/>
                <script type="text/javascript" src="/outsourcing/static/src/js/outsourcing.js"></script>
                <script type="text/javascript" src="/outsourcing/static/src/js/outsourcing_task_kanban_examples.js"></script>
                <script type="text/javascript" src="/outsourcing/static/src/js/tours/outsourcing.js"></script>
                <link rel="stylesheet" type="text/scss" href="/outsourcing/static/src/scss/outsourcing_dashboard.scss"/>
            </xpath>
        </template>

        <template id="assets_frontend" inherit_id="web.assets_frontend" name="Rating outsourcing Assets">
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/scss" href="/outsourcing/static/src/scss/portal_rating.scss"/>
                <script type="text/javascript" src="/outsourcing/static/src/js/portal_rating.js"/>
            </xpath>
        </template>
</odoo>
